{
    "name": "Inform",
    "version": "1.0",
    "description": "An extension that analyzes webpages for subjective content so that you can stay properly informed.",
    "manifest_version": 2,
    "background": {
        "scripts": ["external/jquery-3.4.1.min.js", "background.js"]
    },
    "browser_action": {
        "default_icon": "logo.png"
    },
    "permissions": [
        "activeTab",
        "https://*/"
    ]
}